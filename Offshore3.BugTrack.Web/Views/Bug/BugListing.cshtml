@model List<Offshore3.BugTrack.ViewModels.BugViewModel>


<style>
    #BugItems { list-style-type: none; margin: 0; padding: 0; width: 60%; }
  #BugItems li { margin: 5px auto 5px 20px; border: 1px solid gray;width: 150px;height: 100px; }
</style>

<ul id="BugItems">
    @foreach (var bug in Model)
    {
        <li class="ui-state-default">
            @bug.BugName <br/>
            @bug.AssignerName <br/>
            @bug.UpdateTime <br/>
        </li>
    }
</ul>